{
    "manifest_version": 3,
    "name": "YouTube Playlist Exit",
    "version": "1.0",
    "description": "Automatically removes the playlist context from YouTube videos.",
    "permissions": [
      "tabs"
    ],
    "host_permissions": [
      "https://www.youtube.com/*"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
  